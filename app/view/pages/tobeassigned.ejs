                            <div class="form-group col-md-6">
                                <label for="exampleInputName2"> Product</label>
                                <select class="form-control" id="exampleInputName2">
                                    <option>1 Milles</option>
                                    <option>2 Milles</option>
                                </select>
                            </div>
                            <div class="form-group col-md-6">
